--Create Sequences

CREATE SEQUENCE account_id_seq
START WITH 1
INCREMENT BY 1;

CREATE SEQUENCE reservation_id_seq
START WITH 1
INCREMENT BY 1
MAXVALUE 999999999
NOCYCLE;

CREATE SEQUENCE Location_id_seq
START WITH 1
INCREMENT BY 1
MAXVALUE 999999999
NOCYCLE;

CREATE SEQUENCE user_review_id_seq
START WITH 1
INCREMENT BY 1
MAXVALUE 999999999
NOCYCLE;

CREATE SEQUENCE service_Log_id_seq
START WITH 1
INCREMENT BY 1
MAXVALUE 999999999
NOCYCLE;
